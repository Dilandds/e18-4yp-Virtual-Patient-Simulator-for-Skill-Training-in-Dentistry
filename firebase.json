{
  "functions": {
    "runtime": "nodejs22",
    "region": "us-central1",
    "gen": "2",
    "source": "code/Backend/functions"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
